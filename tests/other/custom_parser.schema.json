{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "root",
  "description": "Some demo structure for demoing.",
  "js2cSettings": {
    "hPrefixFile": "other/custom_parser_h_prefix.inc",
    "cPrefixFile": "other/custom_parser_c_prefix.inc"
  },
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "cType": "int64_t",
      "cParseFunction": "hex_parser",
      "description": "The Identifier",
      "default": "0xABCDEF",
      "maxLength": 8
    },
    "point": {
      "type": "string",
      "cType": "custom_point_t",
      "cParseFunction": "pt_prsr",
      "description": "The coordinates",
      "default": "ab",
      "maxLength": 2
    }
  }
}
