{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "root",
  "description": "Some demo structure for demoing.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "fruits": {
      "type": "array",
      "maxItems": 16,
      "items": {
        "type": "string",
          "maxLength": 19
      }
    },
    "vegetables": {
      "type": "array",
      "maxItems": 16,
      "items": {
          "type": "object",
          "required": [ "name", "is_good" ],
          "additionalProperties": false,
          "properties": {
            "name": {
              "type": "string",
              "description": "The name of the vegetable.",
          "maxLength": 8
            },
            "is_good": {
              "type": "boolean",
              "description": "Is the vegetable any good?"
            }
          }
        }
    },
    "multidimensionals": {
      "type": "array",
      "maxItems": 5,
      "items": {
          "type": "array",
          "maxItems": 7,
          "items": {
              "type": "array",
              "maxItems": 9,
              "items": {
                  "type": "integer"
              }
          }
      }
    }
  }
}
